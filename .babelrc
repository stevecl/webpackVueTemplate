{
  "presets": [
    ["env", {
      // "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8","Android >= 4","iOS >= 7"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false, //自动引入helpers
      "polyfill": false, //自动引入polyfill（core-js提供的polyfill）
      "regenerator": true //自动引入regenerator
    }]
  ],
  "comments": false
}